{
  "name": "bitchify",
  "version": "1.0.3",
  "description": "Bitchify your panty pants",
  "homepage": "https://schascha.github.io/bitchify/",
  "main": "src/js/bitchify.js",
  "scripts": {
    "scss": "node-sass --output-style compressed -o dist/css src/scss",
    "postcss": "postcss -u autoprefixer -r dist/css/*",
    "babel": "cross-env NODE_ENV=production babel src/js/bitchify.js -o dist/js/bitchify.js",
    "uglify": "uglifyjs dist/js/bitchify.js -m -o dist/js/bitchify.min.js",
    "build": "npm test && run-s scss postcss babel uglify",
    "dev": "run-p \"scss -- -w\" \"babel -- -w\"",
    "test": "eslint src/js/*.js && jest src/js --coverage --verbose && sass-lint \"src/scss/**/*.scss\" -v -q"
  },
  "keywords": [
    "javascript",
    "es6",
    "easter-egg"
  ],
  "author": "Sascha KÃ¼nstler <contact.schascha@gmail.com> (http://www.schaschaweb.de)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Schascha/bitchify"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Schascha/bitchify/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^6.7.7",
    "babel-plugin-transform-remove-export": "^1.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-plugin-jest": "^22.2.2",
    "jest": "^24.1.0",
    "node-sass": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "sass-lint": "^1.10.2",
    "uglify-js": "^2.7.4"
  }
}
